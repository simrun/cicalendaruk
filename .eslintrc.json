{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["import"],
  "rules": {
    "import/order": [
      "warn",
      {
        "newlines-between": "always-and-inside-groups",
        "alphabetize": {
          "order": "asc"
        },
        "warnOnUnassignedImports": true
      }
    ]
  }
}
