{
  "extends": [
    "next/core-web-vitals",
    "plugin:next-on-pages/recommended",
    "prettier"
  ],
  "plugins": ["import", "next-on-pages"],
  "rules": {
    "import/order": [
      "warn",
      {
        "newlines-between": "always-and-inside-groups",
        "alphabetize": {
          "order": "asc"
        },
        "warnOnUnassignedImports": true
      }
    ]
  }
}
